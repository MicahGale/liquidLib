# This is a parse file for computing g_r
# g_r code works for both wrapped and unwrapped trajectories.

# program run mode
is_run_mode_verbose         = yes
is_wrapped		   = yes
is_averaged                 = no
add_bar			   = no

# input & output
trajectory_file_name        = abc_test.xyz
trajectory_file_type	   = abc
output_file_name            = abc_new.out

# atomtype in atomgroup
atom_types                   = 0
atom_group                  = system

# reading parameters
dimension                   = 3
start_frame                 = 0
end_frame                   = 3

# computation parameters
bond_parameter_order	   = 6
max_cutoff_length           = 1.2

boxlength		   = 3.0 3.0 3.0
